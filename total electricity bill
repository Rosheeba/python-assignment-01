total=surcharge=amt=0
unit=int(input("enter units consumed= "))
if unit<=50:
    amt=unit*0.50
elif unit<=150:
    amt=25+(unit-50)*0.75
elif unit<=250:
    amt=100+(unit-150)*1.20
else:
    amt=200+(unit-150)*1.50
surcharge=amt*20/100
total=amt+surcharge
print("total electricity bill of rs",total)
